-- #Prop/Trans : #Setoid/Ob/Trans #Prop/El #Prop/Equ

-- args
\ (P1 : #Prop/El) ->
\ (P2 : #Prop/El) ->
\ (P3 : #Prop/El) ->
\ (equ12 : #Prop/Equ P1 P2) ->
\ (equ23 : #Prop/Equ P2 P3) ->

-- res
#Prop/And2/Intro
    (P1->P3) (P3->P1)
    (#.0. P1 P2 P3
        (#Prop/And2/pr1 (P2->P3) (P3->P2) equ23)
        (#Prop/And2/pr1 (P1->P2) (P2->P1) equ12))
    (#.0. P3 P2 P1
        (#Prop/And2/pr2 (P1->P2) (P2->P1) equ12)
        (#Prop/And2/pr2 (P2->P3) (P3->P2) equ23))
