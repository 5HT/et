    \ (x: #Bool/@)
->  \ (y: #List/@ #Nat/@)
-> #Bool/if #Nat/@ x
   (\ (l: #Unit/@) -> #List/length #Nat/@
   (#List/Cons #Nat/@ #Nat/Zero
   (#List/Cons #Nat/@ #Nat/Zero
   (#List/Cons #Nat/@ #Nat/Zero
   (#List/Cons #Nat/@ #Nat/Zero
   (#List/Cons #Nat/@ #Nat/Zero
   (#List/Cons #Nat/@ #Nat/Zero
   (#List/Cons #Nat/@ (#Nat/Succ #Nat/Zero) y))))))))
   (\ (p: #Unit/@) -> #List/length #Nat/@ y)
