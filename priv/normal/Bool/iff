    \ (x: #Bool/@)
->  \ (y: #List/@ #Nat/@)
-> #Bool/if #Nat/@ x
   (\ (l: #Prod0/@) -> #List/length #Nat/@ 
   (#List/Cons #Nat/@ #Nat/Zero 
   (#List/Cons #Nat/@ #Nat/Zero 
